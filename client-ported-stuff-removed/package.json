    "escape-goat": "^4.0.0",
    "fast-equals": "^4.0.3",
    "material-icons": "^1.13.1",
    "normalize.css": "^8.0.1",
    "set-cookie-parser": "^2.5.1",
    "svelte": "^3.55.0",
    "svelte-i18n": "^3.6.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "^1.0.7",
    "@types/cookie": "^0.5.1",
